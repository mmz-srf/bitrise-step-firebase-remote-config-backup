{
  "conditions": [
    {
      "name": "Test iOS Version",
      "expression": "app.id == '1:639365928945:ios:84686408d02173f8f35650' && app.version.>(['1.0'])",
      "tagColor": "BLUE"
    },
    {
      "name": "Long topic names",
      "expression": "device.os == 'ios' && app.id == '1:639365928945:ios:84686408d02173f8f35650'",
      "tagColor": "DEEP_ORANGE"
    },
    {
      "name": "Android Users",
      "expression": "device.os == 'android'",
      "tagColor": "PURPLE"
    },
    {
      "name": "iOSUsers",
      "expression": "device.os == 'ios'",
      "tagColor": "GREEN"
    },
    {
      "name": "AndroidVersion 7.0.0-beta5",
      "expression": "app.id == '1:639365928945:android:f04055667a80369ef35650' && app.version.exactlyMatches(['7.0.0-beta5'])",
      "tagColor": "CYAN"
    },
    {
      "name": "Android 7 DEV",
      "expression": "app.id == '1:639365928945:android:390de6de83ba5b41f35650'",
      "tagColor": "BLUE"
    },
    {
      "name": "iOS 7 Inhouse",
      "expression": "app.id == '1:639365928945:ios:1ea839df97f6d656f35650'",
      "tagColor": "BLUE"
    },
    {
      "name": "android beta6 and above",
      "expression": "app.id == '1:639365928945:android:f04055667a80369ef35650' && app.build.>=(['1112'])",
      "tagColor": "PURPLE"
    },
    {
      "name": "iOS 7 Dev",
      "expression": "app.id == '1:639365928945:ios:14db93d10d8d50b6f35650'",
      "tagColor": "BLUE"
    },
    {
      "name": "Android Beta - DEV",
      "expression": "app.id == '1:639365928945:android:63fdb25f132b879ff35650'",
      "tagColor": "PINK"
    },
    {
      "name": "Android Beta - PROD",
      "expression": "app.id == '1:639365928945:android:f04055667a80369ef35650'",
      "tagColor": "PINK"
    },
    {
      "name": "iOS Beta - PROD",
      "expression": "app.id == '1:639365928945:ios:05b9a9aec749fdfff35650'",
      "tagColor": "PINK"
    },
    {
      "name": "iOS Prod before 7.1.0",
      "expression": "app.id == '1:639365928945:ios:9e5a725bbb31a88bf35650' && app.version.<(['7.1.0'])",
      "tagColor": "TEAL"
    },
    {
      "name": "test",
      "expression": "device.language in ['de']",
      "tagColor": "GREEN"
    }
